{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "description": "Information relating to generation of matrices", 
    "additionalProperties": false, 
    "title": "CRISPRoff", 
    "name": "crisproff", 
    "type": "object", 
    "properties": {
        "describedBy": {
            "description": "The URL reference to the schema.", 
            "type": "string", 
            "pattern": "https://raw.githubusercontent.com/morphic-bio/metadata-schema/main/json_schema/module/protocol/1.0.0/crisproff"
        }, 
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.", 
            "type": "string", 
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$", 
            "example": "4.6.1"
        }, 
        "sgrna_target": {
            "description": "Target of the Single Guide RNAs (sgRNAs)", 
            "type": "string", 
            "user_friendly": "sgRNA target"
        }
    }, 
    "$id": "https://raw.githubusercontent.com/morphic-bio/metadata-schema/main/json_schema/module/protocol/1.0.0/crisproff"
}